{
  "name": "trade-republic-sdk",
  "version": "0.1.0",
  "description": "Unofficial SDK for Trade Republic API",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "bun run example.ts",
    "format": "prettier --write .",
    "prepublishOnly": "npm run build",
    "test": "bun test tests/",
    "test:setup": "bun run tests/test-runner.ts",
    "test:watch": "bun test tests/ --watch"
  },
  "keywords": [
    "trade-republic",
    "trading",
    "api",
    "sdk",
    "finance"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/tr-sdk.git"
  },
  "devDependencies": {
    "@inquirer/prompts": "^7.5.2",
    "@types/bun": "latest",
    "@types/ws": "^8.18.1",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "typescript": "^5"
  },
  "dependencies": {
    "eventemitter3": "^5.0.1",
    "zod": "^3.25.28"
  },
  "peerDependencies": {
    "ws": "^8.0.0"
  },
  "peerDependenciesMeta": {
    "ws": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18"
  }
}
